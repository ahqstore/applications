---
import { readFileSync } from "node:fs";
import { join } from "node:path";

import type { HomeData } from "@/utils/interfaces/home";

import Home from "@/layouts/Home.astro";
import Showcase from "@/components/astro/Showcase.astro";

const check: HomeData = JSON.parse(
  readFileSync(
    join(import.meta.dirname, "../../public/jsondump/home_win32.json")
  ).toString()
);

console.log(check);
---

<Home pageType="windows" homeData={check} footer>
  <Showcase data={check} />
</Home>
